<view><view class="gap"></view><view class="state-area dflex padding bg-main"><view data-event-opts="{{[['tap',[['change',['$0'],[[['stateDatas','',index]]]]]]]}}" class="{{((('item')+' '+'margin-right-sm')+' '+((state==item.name)?'active':''))}}" onTap="__e" a:for="{{stateDatas}}" a:for-item="item" a:for-index="index" a:key="index"><text style="font-size:26rpx;">{{item.name+"("+item.cnt+")"}}</text></view></view><view class="gap"></view><view class="evaluate-area"><view class="padding-lr padding-top bg-main"><view class="eva-box dflex-s padding-bottom-lg" a:for="{{$root.l0}}" a:for-item="item" a:for-index="index" a:key="index"><image class="portrait border-radius-c" src="{{item.$orig.member_headimg}}"></image><view class="right-area flex1 padding-left-sm"><view class="dflex-b ft-dark"><view class="dflex"><text class="name margin-right">{{item.$orig.member_name}}</text><use-rate vue-id="{{'3762e6cc-1-'+index}}" value="{{item.$orig.review_cnt}}" disabled="{{true}}" onVueInit="__l"></use-rate></view><text class="time fs-xs">{{item.g0}}</text></view><view class="fs-sm ft-main padding-top-xs padding-bottom-sm">{{item.$orig.review_content}}</view><view class="dflex dflex-wrap-w"><image src="{{img}}" mode="widthFix" data-event-opts="{{[['tap',[['preview',['$0','$1'],[[['evaluateDatas','',index,'imgs']],[['evaluateDatas','',index],['review_imgs','',i]]]]]]]}}" onTap="__e" a:for="{{item.$orig.review_imgs}}" a:for-item="img" a:for-index="i" a:key="i"></image></view><view><text class="fs-xs ft-dark">{{item.$orig.goods_type||'套餐1'}}</text></view></view></view></view></view></view>