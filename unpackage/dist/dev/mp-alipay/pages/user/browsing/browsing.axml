<view class="{{(('bg-drak')+' '+(empty?'':'padding-tb'))}}"><block a:if="{{empty}}"><use-empty vue-id="7dfd4b58-1" e-style="round" tip="无足迹数据" onVueInit="__l"></use-empty></block><block a:else><view data-event-opts="{{[['tap',[['togoods',['$0'],[[['datas','',index]]]]]]]}}" class="padding-lr" onTap="__e" a:for="{{datas}}" a:for-item="item" a:for-index="index" a:key="index"><view class="product border-radius-sm padding margin-bottom-sm bg-main" style="padding-bottom:15rpx;"><view class="left"><image src="{{item.img}}" mode="aspectFill"></image></view><view class="margin-left-sm pos-r w-full"><text class="clamp-2">{{item.name+" "+item.name_pw}}</text><view class="pos-a dflex-b price-box w-full"><text class="price">{{item.price/100}}</text><view class="dflex-c ft-dark"><view class="dflex-c fs-xs"><text class="margin-xs">浏览</text><text>{{item.visit_cnt}}</text></view><view data-event-opts="{{[['tap',[['deleteBrowsing',['$0'],[[['datas','',index,'_id']]]]]]]}}" class="dflex-c margin-left-sm" catchTap="__e"><text class="iconfont iconlajitong-01 margin-left-xs"></text></view></view></view></view></view></view></block><block a:if="{{!empty&&hasmore}}"><use-loadmore vue-id="7dfd4b58-2" type="{{loadmoreType}}" onVueInit="__l"></use-loadmore></block><use-totop vue-id="7dfd4b58-3" bottom="150" ref="__r" data-ref="usetop" onVueInit="__l"></use-totop><block a:if="{{!empty}}"><view data-event-opts="{{[['tap',[['clear',['$event']]]]]}}" class="fixed-top" onTap="__e"><text class="iconfont iconlajitong-01"></text></view></block></view>