id="79b53a2e-4" onVueInit="__l"></use-copyright><use-action-sheet vue-id="79b53a2e-5" list="{{actionSheetList}}" tips="{{actionSheetTips}}" value="{{actionSheetShow}}" data-event-opts="{{[['^click',[['actionSheetClick']]],['^close',[['actionSheetClose']]],['^input',[['__set_model',['','actionSheetShow','$event',[]]]]]]}}" onClick="__e" onClose="__e" onInput="__e" onVueInit="__l"></use-action-sheet></view>